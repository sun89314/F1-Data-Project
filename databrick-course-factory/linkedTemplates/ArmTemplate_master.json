{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "databrick-course-factory"
		},
		"Databrick Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Databrick Storage'"
		},
		"Databrick Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://723storage.dfs.core.windows.net/"
		},
		"databricksCourse_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0727-065850-q6k6sv5c"
		},
		"trigger-process-f1-data2_properties_master-pipeline_parameters_v_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databrick-course-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Databrick Storage_accountKey": {
						"value": "[parameters('Databrick Storage_accountKey')]"
					},
					"Databrick Storage_properties_typeProperties_url": {
						"value": "[parameters('Databrick Storage_properties_typeProperties_url')]"
					},
					"databricksCourse_properties_typeProperties_existingClusterId": {
						"value": "[parameters('databricksCourse_properties_typeProperties_existingClusterId')]"
					},
					"trigger-process-f1-data2_properties_master-pipeline_parameters_v_window_end_date": {
						"value": "[parameters('trigger-process-f1-data2_properties_master-pipeline_parameters_v_window_end_date')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}